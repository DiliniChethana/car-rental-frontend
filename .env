# Environment Configuration for Spring Boot Backend
REACT_APP_API_URL=http://localhost:8081/api
REACT_APP_APP_NAME=CarRental
REACT_APP_VERSION=1.0.0

# Spring Boot Backend Configuration
REACT_APP_BACKEND_URL=http://localhost:8081
REACT_APP_AUTH_ENDPOINT=/auth
REACT_APP_CARS_ENDPOINT=/cars
REACT_APP_USERS_ENDPOINT=/users

# Fix webpack dev server issue
GENERATE_SOURCEMAP=false
SKIP_PREFLIGHT_CHECK=true
WDS_SOCKET_HOST=localhost
WDS_SOCKET_PORT=3000
REACT_APP_BOOKINGS_ENDPOINT=/bookings

# Optional: Add other configuration variables
# REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
# REACT_APP_STRIPE_PUBLIC_KEY=your_stripe_public_key